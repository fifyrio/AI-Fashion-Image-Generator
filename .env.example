# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
SITE_URL=https://your-site.vercel.app
SITE_NAME=AI Fashion Image Generator

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your_r2_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your_bucket_name

# R2 Public URLs
R2_PUBLIC_BASE_URL=https://pub-xxxxx.r2.dev
R2_MODEL_BASE_URL=https://pub-xxxxx.r2.dev

# KIE API Configuration (Alternative Image Generation Service)
KIE_API_TOKEN=your_kie_api_token_here
KIE_CALLBACK_URL=https://your-site.vercel.app/api/callback

# Image Enhancement API Configuration (Local Python Service)
# URL of the local Python image enhancement service
# Default: http://localhost:8000
IMAGE_ENHANCE_API_URL=http://localhost:8000
